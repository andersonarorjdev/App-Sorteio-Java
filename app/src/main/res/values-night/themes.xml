<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AndersonJunior" parent="Theme.AppCompat">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#C51862</item>
        <item name="colorPrimaryVariant">#9C27B0</item>
        <item name="colorOnPrimary">@color/teal_200</item>

        <!--Define text color primary-->
        <item name="android:textColorPrimary">@color/colorTextos</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/teal_200</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <!--BackGround TOP!-->
        <item name="android:windowBackground">@color/BackgroundTOP</item>
        <item name="android:textColor">@color/colorTextos</item>
    </style>
</resources>